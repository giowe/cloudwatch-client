  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/giowe/cloudwatch-client.git"
  },
  "author": "Giovanni Bruno",
  "contributors": [
    {
      "name": "Giovanni Bruno",
      "email": "giovanni.bruno@soluzionifutura.it",
      "url": "www.soluzionifutura.it"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/giowe/cloudwatch-client/issues"
  },
  "dependencies": {
    "aws-sdk": "^2.81.0",
    "nexe": "^1.1.3",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
